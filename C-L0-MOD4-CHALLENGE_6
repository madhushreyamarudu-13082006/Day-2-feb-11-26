import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ch = new Scanner(System.in);
        long a = ch.nextLong();
        long rev = 0,rem;
        while(a!=0){
            rem = a%10;
            rev = rev*10+rem;
            a = a/10;
        }
        System.out.print(rev);
    }
}
