import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ch = new Scanner(System.in);
        int a = ch.nextInt();
        int rem ;
        while(a!=0){
            rem = a%10;
            System.out.print(rem + " ");
            a=a/10;
        }
       
    }
}
